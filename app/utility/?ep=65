<script type="text/javascript">
					function load_episode_iframe(e, s){
                        var uri = window.location.toString(),
                    	    streamgo = "https://streamgo.me/player/",
                    	    estr = "https://estream.to/embed-",
                    	    opl = "https://openload.co/embed/",
                    	    peight = "https://play.gomovies.sc/8/",
                    	    pten = "https://play.gomovies.sc/10/",
                    	    pseven = "https://play.gomovies.sc/7/";
                    	$("#episode-iframe").removeAttr("src"), $(".btn-eps").removeClass("active"), $("#episodes-server-" + s +" #episode-" + e).addClass("active");
                    	if(s == 10)
							$("#iframe-embed").attr("src", pten + $("#episodes-server-"+s+" #episode-"+ e).data("drive"));
						else if(s == 8)
							$("#iframe-embed").attr("src", peight + $("#episodes-server-"+s+" #episode-"+ e).data("drive"));
						else if(s == 7)
							$("#iframe-embed").attr("src", pseven + $("#episodes-server-"+s+" #episode-"+ e).data("drive"));
						else if(s == 1)
							$("#iframe-embed").attr("src", opl + $("#episodes-server-"+s+" #episode-"+ e).data("openload"));
						else if(s == 2)
							$("#iframe-embed").attr("src", estr + $("#episodes-server-"+s+" #episode-"+ e).data("estream") + ".html");
						else if(s == 6)
							$("#iframe-embed").attr("src", streamgo + $("#episodes-server-"+s+" #episode-"+ e).data("strgo"));
                    	window.history.pushState(null, null, '?ep=' + e + "&sv=" + s);
                    }
					$(document).ready(function () {
						load_episode_iframe(65, 10);
						if (!jQuery.browser.mobile) {
							$('.pas-list ul').perfectScrollbar();
						}

					});
				</script>
